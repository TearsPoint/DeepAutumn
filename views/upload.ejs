        }
        
        a {
            color: #459ae9;
            text-decoration: none;
        }
        
        .change_avatar.dialog_wrp .dialog_bd {
            padding: 0;
            height: 500px;
            text-align: center;
        }
        
        .screen_small .dialog_bd {
